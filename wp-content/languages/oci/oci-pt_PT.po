msgid "Step"
msgstr "Passo"

#: install.php:347
msgid "of"
msgstr "de"

#: install.php:350 install.php:351 install.php:352 install.php:353
msgid "Continue"
msgstr "Continuar"

#: install.php:372
msgid "Select a theme"
msgstr "Selecione um tema"

#: install.php:375
msgid "Select the perfect WordPress theme for your website."
msgstr "Selecione o tema do WordPress perfeito para o seu site."

#: install.php:421
msgid "Preview"
msgstr "Pré-visualizar"

#: install.php:428
msgid "Install"
msgstr "Instalar"

